<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Users</title>
</head>

<body>
	<table>
		<thead>
			<tr>
				<th>Name</th>
				<th>Id</th>
			</tr>
		</thead>
		<tbody>
			<% for (user of users) { %>
				<tr>
					<td>
						<%=user.name%>
					</td>
					<td>
						<%=user.id%>
					</td>
				</tr>
				<%}%>
		</tbody>
	</table>
	<form method="post" action="/update">
		<label for="name">Name</label>
		<input type="text" id="name" name="name">
		<input type="submit">
	</form>
</body>

</html>